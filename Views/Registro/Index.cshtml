@model IEnumerable<AEFINAL.Models.Registro>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Nuevo Registro</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicio)
            </th>
            <th>
                Matricula del vehiculo
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Servicio.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VehiculomatriculaNavigation.Matricula)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NroOrden">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.NroOrden">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.NroOrden">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
